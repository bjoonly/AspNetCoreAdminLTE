@{ Layout = "~/Views/Shared/AdminLTE/_Layout.cshtml";
    ViewData["Title"] = "TC | Delete";}
@model AdminLTE.Models.TerritorialCommunity


<form method="post" asp-action="Delete" asp-controller="TerritorialCommunity">
    <input asp-for="Id" hidden />
    <div class=" p-3 border bg-white my-3 rounded col col-md-10 col-lg-9 mx-auto">
        <div class="form-group row ">
            <h2 class="col">Delete territorial community</h2>
        </div>
        <div class="row">
            <div class="col">
                <div class="row">
                    <div class="form-group col ">
                        <label asp-for="Name"></label>
                        <input asp-for="Name" disabled class="form-control" />

                    </div>
                    <div class="form-group col  ">
                        <label asp-for="Area"></label>
                        <input asp-for="Area" disabled class="form-control" />

                    </div>
                </div>
                <div class="form-group col px-0 pt-2 mb-0">
                    <div class="row">
                        <div class="col">
                            <input type="submit" class="btn btn-danger  col " value="Delete" />
                        </div>
                        <div class="col text-right">
                            <a asp-action="Index" class="btn btn-info col "> Back</a>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</form>
